{
	"name": "satisync",
	"version": "0.0.1",
	"description": "Sync Satisfactory saves",
	"author": "Kid Krule",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "electron-webpack dev",
		"build": "electron-webpack",
		"electron:pack": "electron-builder --dir -c.compression=store -c.mac.identity=null",
		"electron:dist": "electron-builder -mw",
		"build:app": "npm-run-all --sequential build electron:pack",
		"dist": "npm-run-all --sequential build electron:dist",
		"postinstall": "electron-builder install-app-deps",
		"lint": "eslint --ext .js src"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@babel/register": "^7.6.2",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.2.0",
		"electron": "^15.5.5",
		"electron-builder": "^21.2.0",
		"electron-reload": "^1.5.0",
		"electron-webpack": "^2.7.4",
		"eslint": "^6.5.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.2.0",
		"file-loader": "^4.2.0",
		"node-sass": "^4.13.0",
		"npm-run-all": "^4.1.5",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9"
	},
	"dependencies": {
		"@coriou/react-easy-store": "^0.0.6",
		"archiver": "^3.1.1",
		"chokidar": "^3.2.2",
		"data-forge": "^1.7.7",
		"dayjs": "^1.8.16",
		"electron-window-state": "^5.0.3",
		"filesize": "^5.0.3",
		"mobx": "^5.14.2",
		"purecss-sass": "^1.0.1",
		"react": "16.11.0",
		"react-dom": "16.11.0",
		"react-router-dom": "^5.1.2",
		"source-map-support": "^0.5.13",
		"transfer-sh": "^2.1.1"
	},
	"build": {
		"appId": "com.kidkrule.satisync",
		"productName": "Satisync",
		"artifactName": "${productName}-${version}.${ext}",
		"copyright": "Copyright Â© 2018 ${author}"
	},
	"electronWebpack": {
		"renderer": {
			"webpackConfig": "webpack.renderer.additions.js"
		}
	}
}
